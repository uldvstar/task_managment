        "gulp-sass": "^3.1.0",
        "gulp-streamify": "^1.0.2",
        "gulp-uglify": "^3.0.0",
        "gulp-uglify-es": "^1.0.4",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.13",
