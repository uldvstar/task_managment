        "vue": "^2.6.10",
        "vue-avatar": "^2.2.0",
        "vue-template-compiler": "^2.6.10",
        "vuelidate": "^0.7.4",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "axios": "^0.19",
